<root>
    <styles>
        <include src="file://{resources}/styles/custom_game/top_bar_hud_buttons/top_bar_hud_buttons.css"/>
    </styles>
    <scripts>
        <include src="file://{resources}/scripts/custom_game/top_bar_hud_buttons/top_bar_hud_buttons.js"/>
    </scripts>
    <Panel class="Root" hittest="false" hittestchildren="true">
        <Panel id="ButtonsContainer" hittest="true" hittestchildren="true">
			<Button id="DashboardButton" onactivate="OnDashboardButtonClicked()" onmouseover="UIShowTextTooltip( #DOTA_HUD_BackToDashboard )" onmouseout="UIHideTextTooltip()" />
			<Button id="SettingsButton" onactivate="OnSettingsButtonPressed()" onmouseover="UIShowTextTooltip( #DOTA_HUD_Settings )" onmouseout="UIHideTextTooltip()" />
			<Button id="ToggleScoreboardButton" onactivate="OnScoreboardButtonPressed()" onmouseover="UIShowTextTooltip( #DOTA_HUD_Scoreboard )" onmouseout="UIHideTextTooltip()" />
            <Button id="DiscordButton" onactivate="OnDiscordButtonPressed()" onmouseover="UIShowTextTooltip( #ui_top_hud_buttons_discord )" onmouseout="UIHideTextTooltip()" />
            <Button id="DebugButton" class="Hidden" onactivate="OnDebugButtonPressed()" onmouseover="UIShowTextTooltip( #ui_top_hud_buttons_debug_panel )" onmouseout="UIHideTextTooltip()" />
            <Button id="PlayersTopButton" onactivate="OnPlayersTopButtonPressed()" onmouseover="UIShowTextTooltip( #ui_top_hud_buttons_players_top_panel )" onmouseout="UIHideTextTooltip()" />
            <Button id="DonationPanelButton" onactivate="OnDonationPanelButtonPressed()" onmouseover="UIShowTextTooltip( #ui_top_hud_buttons_donation_panel )" onmouseout="UIHideTextTooltip()" />
        </Panel>
    </Panel>
</root>