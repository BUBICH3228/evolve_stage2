			<Label id="BossesRespawnTimeModifierLabel" class="ValueLabel" text="0" html="true" />
      </Panel>
      <Panel class="IconValuePair">
         <Panel class="IconWrapper">
            <Image class="Icon" src="file://{images}/custom_game/ui/lobby_settings/stats/bat.png" scaling="stretch-to-fit-preserve-aspect"/>
         </Panel>
         <Label class="ModifierTooltip" text="#ui_lobby_settings_enemies_tower_upgrade_time"/>
			<Label id="TowerUpgradeTimeModifierLabel" class="ValueLabel" text="0" html="true" />
