      </Panel>
      <Panel class="IconValuePair">
         <Panel class="IconWrapper">
            <Image class="Icon" src="file://{images}/custom_game/ui/lobby_settings/stats/exp.png" scaling="stretch-to-fit-preserve-aspect"/>
         </Panel>
         <Label class="ModifierTooltip" text="#ui_lobby_settings_enemies_bonus_XP"/>
			<Label id="BonusXPModifierLabel" class="ValueLabel" text="0%" html="true" />
      </Panel>
      <Panel class="IconValuePair">
