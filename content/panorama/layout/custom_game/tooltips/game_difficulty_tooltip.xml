<root>
   <styles>
        <include src="s2r://panorama/styles/custom_game/tooltips/game_difficulty_tooltip.css" />
   </styles>
   <scripts>
      <include src="file://{resources}/scripts/custom_game/tooltips/game_difficulty_tooltip.js" />
   </scripts>
   <Panel class="TooltipContent" hittest="false" ontooltiploaded="OnTooltipLoaded()">
      <Panel class="IconValuePair">
         <Panel class="IconWrapper">
            <Image class="Icon" src="file://{images}/custom_game/ui/lobby_settings/stats/health.png" scaling="stretch-to-fit-preserve-aspect"/>
         </Panel>
         <Label class="ModifierTooltip" text="#ui_lobby_settings_enemies_max_hp"/>
			<Label id="HealthModifierLabel" class="ValueLabel" text="0%" html="true" />
      </Panel>
      <Panel class="IconValuePair">
         <Panel class="IconWrapper">
            <Image class="Icon" src="s2r://panorama/images/hud/reborn/icon_damage_psd.vtex" scaling="stretch-to-fit-preserve-aspect"/>
         </Panel>
         <Label class="ModifierTooltip" text="#ui_lobby_settings_enemies_attack_damage"/>
			<Label id="AttackDamageModifierLabel" class="ValueLabel" text="0%" html="true" />
      </Panel>
      <Panel class="IconValuePair">
         <Panel class="IconWrapper">
            <Image class="Icon" src="file://{images}/custom_game/ui/lobby_settings/stats/spell_amp.png" scaling="stretch-to-fit-preserve-aspect"/>
         </Panel>
         <Label class="ModifierTooltip" text="#ui_lobby_settings_enemies_spell_amplification"/>
			<Label id="SpellAmplificationModifierLabel" class="ValueLabel" text="0%" html="true" />
      </Panel>
      <Panel class="IconValuePair">
         <Panel class="IconWrapper">
            <Image class="Icon" src="file://{images}/custom_game/ui/lobby_settings/stats/bonus_gold.png" scaling="stretch-to-fit-preserve-aspect"/>
